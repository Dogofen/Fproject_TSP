#ifndef __ADTDEFS_H__
#define __ADTDEFS_H__

typedef enum ADTErr
{
	/*general ADT Errors*/
	ERR_OK=0,
	ERR_GENERAL,
	ERR_NOT_INITIALIZED,
	ERR_ALLOCATION_FAILED,
	ERR_REALLOCATION_FAILED,
	ERR_UNDERFLOW,
	ERR_OVERFLOW,
	ERR_WRONG_INDEX,
	ERR_WRON_INPUT,
	ERR_NOT_FOUND,
	ERR_DUPLICATIONS

}ADTErr;

char* GetErrorName(ADTErr i);

#endif
